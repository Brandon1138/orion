{
	"name": "orion",
	"version": "1.0.0",
	"description": "Daily planning copilot + local file/OS agent",
	"type": "module",
	"engines": {
		"node": ">=20.10.0 <22.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "node scripts/start-dev.js",
		"build": "npm run build --workspaces",
		"test": "vitest",
		"test:e2e": "npm run test:e2e --workspaces",
		"lint": "eslint . --max-warnings 0",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"check": "npm run format:check && npm run lint",
		"fix": "npm run format && npm run lint:fix",
		"lint:workspaces": "npm run lint --workspaces",
		"audit:security": "npm audit --audit-level high",
		"audit:outdated": "npm outdated --long",
		"update:check": "npm-check-updates --interactive",
		"dependency:matrix": "node scripts/check-compatibility.js"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.56.0",
		"googleapis": "^126.0.0",
		"ical.js": "^2.0.1",
		"openai": "^4.50.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.10.0",
		"@types/pg": "^8.11.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.3",
		"npm-check-updates": "^17.0.0",
		"prettier": "^3.6.2",
		"typescript": "^5.3.0",
		"typescript-eslint": "^8.38.0",
		"vite": "^5.0.0",
		"vitest": "^1.6.0"
	},
	"overrides": {
		"@openai/agents": "^0.0.14",
		"@modelcontextprotocol/sdk": "^1.16.0"
	},
	"keywords": [
		"ai",
		"calendar",
		"planning",
		"agent",
		"productivity"
	],
	"author": "Brandon Aron",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/your-org/orion.git"
	}
}
